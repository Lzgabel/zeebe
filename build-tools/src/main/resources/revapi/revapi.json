[
  {
    "extension": "revapi.semver.ignore",
    "configuration": {
      "enabled": true,
      "versionIncreaseAllows": {
        "major": "breaking",
        "minor": "nonBreaking",
        "patch": "nonBreaking"
      },
      "passThroughDifferences": [
        "java.class.nonPublicPartOfAPI"
      ]
    }
  },
  {
    "extension": "revapi.java",
    "configuration": {
      "reportUsesFor": "all-differences",
      "matchOverloads": false
    }
  }
]
